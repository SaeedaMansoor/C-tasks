#include<iostream>
using namespace std;
int main(){
	int temp;
	int n; //array size	
	cout<<"enter the size of array="<<endl;
	cin>>n;
	int num[n];  //array name
	
	
	for(int i=0;i<n;i++)
	{cin>>num[i];
	}
	int *p=num;
	 //cout<<*p;
	for(int z=0;z<n;z++)
	
	{for(int j=z+1;j<n;j++)
	{
	 if(num[z]>num[j])
	
	{ temp=p[z];
p[z]=p[j];
	p[j]=temp;
	
	}
}
	}
	



	for(int a=0;a<n;a++)


	{cout<<*p;
p++;
	}
	return 0;
	
	
	
	
}


// This will print the address of variable var
//cout<<&var;    

